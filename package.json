{
  "name": "find-and-replace-immutable",
  "version": "0.0.2",
  "description": "Exposes immutable findAndReplace and replaceAt functions on top of lodash/fp",
  "main": "lib/index.js",
  "repository": "https://github.com/robertherber/find-and-replace-immutable",
  "author": "Robert Herber <robert@herber.me>",
  "license": "MIT",
  "keywords": [
    "lodash",
    "immutable",
    "lodash-fp",
    "find-and-replace",
    "replace-at",
    "safe",
    "functional",
    "utility",
    "helper"
  ],
  "scripts": {
    "test": "eslint src && jest src",
    "build": "babel src --out-dir lib --source-maps inline",
    "prepublish": "yarn test && yarn run build"
  },
  "files": [
    "lib"
  ],
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react-native": "^2.0.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-config-prettier": "^2.2.0",
    "eslint-plugin-flowtype": "^2.34.0",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jest": "^20.0.3",
    "eslint-plugin-json": "^1.2.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-lodash-fp": "^2.1.3",
    "eslint-plugin-prettier": "^2.1.2",
    "eslint-plugin-react": "^7.1.0",
    "jest": "^20.0.4",
    "prettier": "^1.4.4"
  },
  "dependencies": {
    "lodash": "^4.17.4"
  },
  "jest": {
    "collectCoverage": true,
    "coverageReporters": [
      "json",
      "lcov",
      "text",
      "cobertura"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js}",
      "!**/node_modules/**"
    ]
  }
}
